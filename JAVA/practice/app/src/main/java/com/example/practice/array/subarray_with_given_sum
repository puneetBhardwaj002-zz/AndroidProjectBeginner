#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,n,s;
    cin>>t;
    while(t--){
        cin>>n>>s;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
            if(i !=0 )
                a[i] += a[i-1];
        }
        int i=0,k=1,flag=0,ll=-999,hl=-999;
        for(int i=0;i<n;i++){
            if(a[i] == s){
                ll=1;
                hl=i+1;
                flag=1;
            }
        }
        if(!flag){
            while(i<n){
            if((a[k]-a[i]) ==s){
                ll=i+2;
                hl=k+1;
                break;
            }else if(a[k]-a[i] >s){
                i++;
                k=i+1;
            }else{
                k++;
            }
        }
        }
        if(hl == -999 && ll== -999){
            cout<<"-1"<<endl;
        }else{
            cout<<ll<<" "<<hl<<endl;
        }
    }
	return 0;
}

